# mkinitcpio preset file for linux-vserver

ALL_config="/etc/mkinitcpio.conf"
ALL_kver="/boot/vmlinuz-linux-vs"

PRESETS=('default' 'fallback')

#default_config="/etc/mkinitcpio.conf"
default_image="/boot/initramfs-linux-vs.img"
#default_options=""

#fallback_config="/etc/mkinitcpio.conf"
fallback_image="/boot/initramfs-linux-vs-fallback.img"
fallback_options="-S autodetect"
